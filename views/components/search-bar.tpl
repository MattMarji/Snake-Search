      <div class="search-col col-lg-12">
        <div id="query-group" class="input-group">
          <input id="query" type="text" name="keywords" type="text" class="form-control" placeholder="Search for..." required>
        </div><!-- /input-group -->
      </div><!-- /.col-lg-6 -->
    </div><!-- /.row -->
    <div style="margin-bottom:6%;" class="row">
        <div id="query-container">
            <input id="feeling-lucky"  value="Feeling Lucky" class="btn btn-lg btn-primary center-block" />  
